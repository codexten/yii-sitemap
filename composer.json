{
  "name": "codexten/yii-sitemap",
  "description": "yii sitemap",
  "type": "cms-package",
  "require": {
    "zhelyabuzhsky/yii2-sitemap": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\sitemap\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "sitemap": "config/sitemap.php",
      "console": [
        "$sitemap",
        "config/console.php"
      ],
      "params": "config/params.php"
    }
  }
}
